{
  "expo": {
    "name": "Grupo 3D",
    "slug": "grupo3d",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/logo/logoFundoBranco.png",
    "scheme": "grupo3d",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.grupo3d.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logo/logoFundoTransparente.png",
        "backgroundColor": "#000000"
      },
      "package": "com.grupo3d.app",
      "edgeToEdgeEnabled": true,
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/logo/logoFundoBranco.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/logo/logoFundoTransparente.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "61c7582c-90dc-422e-8b26-9a52dd8b7959"
      }
    }
  }
}
